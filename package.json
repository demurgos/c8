{
  "name": "c8",
  "version": "2.0.0",
  "description": "Collect test coverage using v8's inspector",
  "bin": "./build/bin/c8.js",
  "scripts": {
    "build:lib": "tsc --project src/lib/tsconfig.json",
    "bundle": "bundle-dependencies update",
    "test": "npm run build:lib && nyc mocha --require ts-node/register \"./src/test/*.ts\"",
    "posttest": "standard",
    "release": "standard-version"
  },
  "c8": {
    "exclude": [
      "test/*.js"
    ]
  },
  "standard": {
    "ignore": [
      "build",
      "src/test/fixtures"
    ]
  },
  "keywords": [
    "coverage",
    "v8",
    "test",
    "istanbul",
    "profiler",
    "inspector"
  ],
  "engines": {
    "node": ">=8.8.1"
  },
  "author": "Ben Coe <ben@npmjs.com>",
  "contributors": [
    "Charles Samborski <demurgos@demurgos.net> (demurgos.net)"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/find-up": "^2.1.1",
    "@types/mkdirp": "^0.5.2",
    "@types/rimraf": "^2.0.2",
    "@types/uuid": "^3.4.3",
    "@types/yargs": "^11.1.1",
    "chrome-remote-interface": "^0.26.1",
    "demurgos-spawn-wrap": "^2.0.0-beta.2",
    "devtools-protocol": "0.0.581326",
    "find-up": "^3.0.0",
    "foreground-child": "^1.5.6",
    "istanbul-lib-coverage": "^2.0.1",
    "istanbul-lib-report": "^2.0.1",
    "istanbul-reports": "^2.0.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "signal-exit": "^3.0.2",
    "test-exclude": "^5.0.0",
    "uuid": "^3.1.0",
    "v8-to-istanbul": "^1.2.0",
    "yargs": "^12.0.1",
    "yargs-parser": "^10.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.6",
    "bundle-dependencies": "^1.0.2",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "standard": "^11.0.1",
    "standard-version": "^4.2.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "bundledDependencies": [
    "find-up",
    "foreground-child",
    "istanbul-lib-coverage",
    "istanbul-lib-report",
    "istanbul-reports",
    "mkdirp",
    "rimraf",
    "signal-exit",
    "spawn-wrap",
    "test-exclude",
    "uuid",
    "v8-to-istanbul",
    "yargs",
    "yargs-parser"
  ]
}
